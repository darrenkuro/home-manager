{
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "git.autofetch": true,
    "editor.suggestOnTriggerCharacters": false,
    "explorer.confirmDelete": false,
    "editor.accessibilitySupport": "off",
	"git.confirmSync": false,
    "editor.defaultFormatter": "evilcat.norminette-42",
    "workbench.startupEditor": "none",
    "explorer.confirmDragAndDrop": false,
    "prettier.singleAttributePerLine": true,
    "prettier.tabWidth": 4,
    "material-icon-theme.activeIconPack": "react",
    "material-icon-theme.opacity": 0.8,
    "material-icon-theme.saturation": 0.8,
    "material-icon-theme.folders.associations": {
        "modals": "interface",
        "commands": "functions",
        "component": "react-components",
        "to-process": "error",
        "data-fetching": "Api",
        "data-models": "database",
        "entry-model": "database",
        "experiment": "project"
    },
    "material-icon-theme.files.associations": {
        "svg.tsx": "svgr",
        "constants.ts": "tsconfig",
        "playground.ts": "test-ts"
    },
    "files.exclude": {
        "**/Icon?": true
    },
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": false,
        "yaml": false,
        "plaintext": true,
        "markdown": false
    },
    "prettier.bracketSameLine": true,
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "42header.username": "dlu",
	"42header.email": "dlu@student.42berlin.de",
    "editor.insertSpaces": false,
	"editor.detectIndentation": false,
	"separators.classes.borderStyle": "dashed",
	"workbench.editor.empty.hint": "hidden",
	"typescript.enablePromptUseWorkspaceTsdk": true,
	"github.copilot.nextEditSuggestions.enabled": true
}
